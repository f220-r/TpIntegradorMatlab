clear
clc

t0 = 0;   % Tiempo Inicial
tF = 60;   % Tiempo Final
X0 = [0 0 0]; % Condición inicial
h = 10e-3;   % Intervalos de tiempos
M = (tF-t0)/h;  % Pasos de solución

[T,Y] = RungeKuttaOrdenN('Ejercicio1_a',t0,tF,X0,M);
Y = 1e2 .* Y(:,1);
I = Trapezoidal_Compleja(T,Y)';
string_ = {'Area = %d',I}
plot(T,Y,'g');
hold on
area(T,Y);
legend('Area = ');
hold off;
ylabel('y (cm)');
xlabel('t (s)');
title('A)');
grid
